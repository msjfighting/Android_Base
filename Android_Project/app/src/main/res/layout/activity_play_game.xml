<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


   <include layout="@layout/navbar" android:id="@+id/nav"/>




   <com.msj.android_project.view.GamePintuLayout
       android:id="@+id/id_pintu"

       android:layout_centerInParent="true"
       android:padding="3dp"
       android:layout_width="fill_parent"
       android:layout_height="fill_parent">

   </com.msj.android_project.view.GamePintuLayout>


   <RelativeLayout
       android:padding="5dp"
       android:layout_below="@+id/nav"
       android:layout_margin="3dp"
       android:layout_above="@id/id_pintu"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content">
      <TextView
          android:id="@+id/id_level"
          android:gravity="center"
          android:padding="4dp"
          android:textColor="@color/white"
          android:background="@drawable/item_bg_round"
          android:layout_width="70dp"
          android:layout_height="70dp"/>


      <TextView
          android:layout_alignParentRight="true"
          android:id="@+id/id_time"
          android:gravity="center"
          android:padding="4dp"
          android:textColor="@color/white"
          android:background="@drawable/item_bg_round"
          android:layout_width="70dp"
          android:layout_height="70dp"/>

   </RelativeLayout>

</RelativeLayout>